<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mohsin.contactdemo">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity
            android:name=".ContactActivity.View.SplashScreen"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditContact.EditContact"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".History.View.History"
            android:screenOrientation="sensorPortrait"></activity>
        <activity
            android:name=".ContactActivity.View.MainActivity"
            android:screenOrientation="sensorPortrait"></activity>
    </application>

</manifest>